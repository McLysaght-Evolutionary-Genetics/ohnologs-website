{
  "name": "ohnologs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && vite dev",
    "build": "prisma generate && svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "prisma generate && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "prisma generate && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/adapter-node": "^1.1.7",
    "@sveltejs/kit": "^1.5.2",
    "@types/d3": "^7.4.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "carbon-components-svelte": "^0.72.2",
    "carbon-icons-svelte": "^11.4.0",
    "d3": "^7.8.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "newick-js": "^1.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prisma": "^4.10.0",
    "sass": "^1.58.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "tailwindcss": "^3.2.6",
    "tippy.js": "^6.3.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "vite": "^4.1.1",
    "zod": "^3.20.6",
    "zod-prisma": "^0.5.4"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^4.10.0",
    "phylotree": "^1.2.3"
  }
}
